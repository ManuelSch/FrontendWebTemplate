{
  "name": "FrontendWebTemplate",
  "authors": [
    "manuel.schueller@tuwien.ac.at"
  ],
  "description": "",
  "main": "",
  "license": "MIT",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "^3.1.1",
    "angular": "^1.6.1",
    "glyphicons-only-bootstrap": "^0.0.1",
    "normalize-css": "normalize.css#^5.0.0",
    "angular-route": "bower-angular-route#^1.6.1",
    "angular-resource": "^1.6.1"
  },
  "dependencies_currently_not_used": {
    "bootstrap": "^3.3.7"
  },
  "install": {
    "path": {
      "js": "./src/lib/{name}/js",
      "css": "./src/lib/{name}/css",
      "eot": "./src/lib/{name}/fonts",
      "svg": "./src/lib/{name}/fonts",
      "ttf": "./src/lib/{name}/fonts",
      "woff": "./src/lib/{name}/fonts",
      "woff2": "./src/lib/{name}/fonts",
      "*": "./src/lib/{name}"
    },
    "sources": {
      "jquery": [
        "bower_components/jquery/dist/jquery.js"
      ],
      "angular": [
        "bower_components/angular/angular.js",
        "bower_components/angular/angular-csp.css"
      ],
      "glyphicons-only-bootstrap": [
        "bower_components/glyphicons-only-bootstrap/css/bootstrap.css",
        "bower_components/glyphicons-only-bootstrap/css/bootstrap-theme.css",
        "bower_components/glyphicons-only-bootstrap/dist/fonts/glyphicons-halflings-regular.eot",
        "bower_components/glyphicons-only-bootstrap/dist/fonts/glyphicons-halflings-regular.svg",
        "bower_components/glyphicons-only-bootstrap/dist/fonts/glyphicons-halflings-regular.ttf",
        "bower_components/glyphicons-only-bootstrap/dist/fonts/glyphicons-halflings-regular.woff",
        "bower_components/glyphicons-only-bootstrap/dist/fonts/glyphicons-halflings-regular.woff2"
      ],
      "normalize-css": [
        "bower_components/normalize-css/normalize.css"
      ],
      "angular-route": [
        "bower_components/angular-route/angular-route.js"
      ],
      "angular-resource": [
        "bower_components/angular-resource/angular-resource.js"
      ]
    },
    "sources_currently_not_used": {
      "bootstrap": [
        "bower_components/bootstrap/dist/css/bootstrap.css",
        "bower_components/bootstrap/dist/css/bootstrap-theme.css",
        "bower_components/bootstrap/dist/fonts/glyphicons-halflings-regular.eot",
        "bower_components/bootstrap/dist/fonts/glyphicons-halflings-regular.svg",
        "bower_components/bootstrap/dist/fonts/glyphicons-halflings-regular.ttf",
        "bower_components/bootstrap/dist/fonts/glyphicons-halflings-regular.woff",
        "bower_components/bootstrap/dist/fonts/glyphicons-halflings-regular.woff2",
        "bower_components/bootstrap/dist/js/bootstrap.js"
      ]
    }
  }
}
